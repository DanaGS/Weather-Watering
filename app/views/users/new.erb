<h1>Register</h1>
<%= form_with model: @user, url: register_path do |f|%>
  <% if @user.errors.any? %>
    <div>
      <% @user.errors.full_messages.each do |message|%>
        <div><%= message%></div>
      <% end %>
    </div>
  <% end %>
  <div>
    <%= f.label :email%>
    <%= f.email_field :email%>
  </div>
  <div>
    <%= f.label :password%>
    <%= f.password_field :password%>
  </div>
  <div>
    <%= f.label :password_confirmation%>
    <%= f.password_field :password_confirmation%>
  </div>
  <div>
    <%= f.submit "Get Started!"%>
  </div>
<% end %>
